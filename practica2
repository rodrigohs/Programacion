#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"
using namespace std;
void main(){
	int i, x, n, suma;
	cout << "ingrese la cantidad de numeros" << endl;
	cin >> n;
	suma = 0;
	for (i = 1; i <= n; i++){
		x = pow(i, 2);
		suma = suma + x;
		
	}
	cout << "la suma es" << suma << endl;
	_getch();
}


#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"

using namespace std;

float Raiz (float p);

void main ()
{
	float n,r;
	cout<<"ingrese el numero a calcular la raiz cuadrada: "<<endl;
	cin>>n;
	r=Raiz(n);
	cout<<"la raiz cuadrada de "<<n<<"es: "<<Raiz(n);
	getch();
}

float Raiz(float p)
{ 
	return sqrt(p);
}

